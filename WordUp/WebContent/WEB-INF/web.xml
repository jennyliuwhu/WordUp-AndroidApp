<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>WordUp</display-name>
  <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>org.wordup.register</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>org.wordup.login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>search</servlet-name>
    <servlet-class>org.wordup.search</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>search</servlet-name>
    <url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>note</servlet-name>
    <servlet-class>org.wordup.note</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>note</servlet-name>
    <url-pattern>/note</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>recommend</servlet-name>
    <servlet-class>org.wordup.recommend</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>recommend</servlet-name>
    <url-pattern>/recommend</url-pattern>
  </servlet-mapping>
</web-app>